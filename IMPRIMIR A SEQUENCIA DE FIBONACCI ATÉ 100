package com.mycompany.fibonacci;

/* IMPRIMIR A SEQUENCIA DE FIBONACCI ATÉ 100
 *
 * @author Belmont
*/

public class Fibonacci {
    public static void main (String[] arg){
        int x = 0;
        int y = 1;
        System.out.print(x+", ");
        while (y<100){
            System.out.print(y+", ");
            y = x + y;
            x = y-x;
        }
    }
    
}
